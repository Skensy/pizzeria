{
  "kind": "collectionType",
  "collectionName": "menu_items",
  "info": {
    "singularName": "menu-item",
    "pluralName": "menu-items",
    "displayName": "Menu Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nome": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "pizze",
        "sfizi",
        "dolci",
        "bevande"
      ]
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "isVegetarian": {
      "type": "boolean",
      "default": false
    },
    "isSpicy": {
      "type": "boolean",
      "default": false
    }
  }
}
